# Backlight
"xbacklight -dec 5"
    XF86MonBrightnessDown
"xbacklight -inc 5"
    XF86MonBrightnessUp

# Sound
"amixer -q -D pulse set Master 5%+; pkill -SIGRTMIN+10 i3blocks"
    XF86AudioRaiseVolume
"amixer -q -D pulse set Master 5%-; pkill -SIGRTMIN+10 i3blocks"
    XF86AudioLowerVolume
"amixer -q -D pulse set Master toggle; pkill -SIGRTMIN+10 i3blocks"
    XF86AudioMute

# Playback
"playerctl play-pause"
    XF86AudioPlay
"playerctl next"
    XF86AudioNext
"playerctl previous"
    XF86AudioPrev

