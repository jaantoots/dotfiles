# Backlight
"xbacklight -time 50 -dec 5; pkill -SIGRTMIN+11 i3blocks"
    XF86MonBrightnessDown
"xbacklight -time 50 -inc 5; pkill -SIGRTMIN+11 i3blocks"
    XF86MonBrightnessUp

# Sound
"amixer -q -D pulse set Master 5%+; pkill -SIGRTMIN+10 i3blocks"
    XF86AudioRaiseVolume
"amixer -q -D pulse set Master 5%-; pkill -SIGRTMIN+10 i3blocks"
    XF86AudioLowerVolume
"amixer -q -D pulse set Master toggle; pkill -SIGRTMIN+10 i3blocks"
    XF86AudioMute

# Playback
"playerctl play-pause; pkill -SIGRTMIN+12 i3blocks"
    XF86AudioPlay
"playerctl next; pkill -SIGRTMIN+12 i3blocks"
    XF86AudioNext
"playerctl previous; pkill -SIGRTMIN+12 i3blocks"
    XF86AudioPrev

